<head>
	<title>LineHive: what up</title>
	<%= default_javascript %>
	<%= stylesheet_link_tag "fullscreen" %>
</head>


<div id="timeline">
	<iframe frameborder="0" width="900" height="300" src="http://linehive.com/embed/79"></iframe> 
	<div id="Bcontrols">
		<img id="prev" src="/images/fullscreen/left.png">
		<img id="next" src="/images/fullscreen/right.png">
	</div>
	<div id="Rcontrols">
		<div id="prev" >Prev. line</div>
		<div id="next" >Next line</div>
	</div>
	<div class="timelineIndicator" style=""><img src="data:image/png;base64,">
<p><b>LineHive</b>: <i>Plane crash in East Palo Alto causes power outage</i>...<br>
<a>10 related lines</a> <a href="#" id="close">close</a></p>
	</div>
</div>
	
<div class="timelineIndicator" style="">
	<img src="data:image/png;base64,">
	<p><b>LineHive</b>: <i>Plane crash in East Palo Alto causes power outage</i>...<br>
	<a>10 related lines</a> <a href="#" id="close">close</a></p>
</div>

<div id="source">
	<iframe frameborder="0" id="page" name="page" src="http://linehive.com/">
	</iframe>
</div>
<script>
	$(document).ready(function() {
		$(".timelineIndicator").mouseenter(function(){
			//$('#timeline').show();
			$('#timeline').slideDown("slow");
			//$("#timelineIndicator").css({"top":315});
		});
	$("#source").mouseenter(function(){
			$('#timeline').hide();
			$("#timelineIndicator").css({"top":0});
		});
	
	$("#close").click(function(){
			$('#timeline').hide();
			$('#timelineIndicator').hide();
		});
	});
</script>
<div class="space_20"></div>

